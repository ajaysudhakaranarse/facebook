<h1>All Users</h1>
<table border="1">
  <%@user.each do |user|%>
  <tr>
    <td><%=user.name%></td>
    <td><%=user.password%></td>
    <td><%=user.gender%></td>
    <td><%=user.email%></td>
    <td><%=user.date_of_birth%></td>
    <td><%=link_to 'Profile', user_path(user)%>
    <td><%=link_to 'Edit', edit_user_path(user)%>
    <td><%=link_to 'Delete', user, method: :delete, data: { confirm: 'Are you sure?'}%></td>
  </tr>
  <%end%>
</table>
